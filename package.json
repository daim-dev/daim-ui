{
  "name": "daim-ui",
  "version": "0.0.1",
  "description": "Vue component library styled with Uno css",
  "main": "./dist/daim-ui.umd.js",
  "module": "./dist/daim-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/daim-ui.es.js",
      "require": "./dist/daim-ui.umd.js"
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore .",
    "lint:fix": "yarn run lint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daim-dev/daim-ui.git"
  },
  "keywords": [
    "vue",
    "vite",
    "unocss",
    "component",
    "library"
  ],
  "author": "Damien Robinson <damien.robinson@daim.dev> (https://daim.dev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daim-dev/daim-ui/issues"
  },
  "homepage": "https://github.com/daim-dev/daim-ui#readme",
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "9.0.0",
    "@nuxtjs/eslint-config-typescript": "9.0.0",
    "@types/lodash": "4.14.181",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "@unocss/reset": "0.30.11",
    "@vitejs/plugin-vue": "^2.3.0",
    "@vue/compiler-sfc": "3.2.31",
    "@vue/composition-api": "1.4.9",
    "@vue/eslint-config-standard": "6.1.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-nuxt": "3.2.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.5.0",
    "prettier": "2.6.2",
    "typescript": "^4.5.4",
    "unocss": "0.30.11",
    "unplugin-vue-components": "0.18.5",
    "vite": "^2.9.0",
    "vite-plugin-vue2": "1.9.3",
    "vue": "^2.6.0",
    "vue-template-compiler": "2.6.14",
    "vue-tsc": "^0.29.8",
    "vue3": "npm:vue@3"
  },
  "packageManager": "yarn@3.2.0"
}
