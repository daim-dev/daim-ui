{
  "name": "daim-ui",
  "version": "1.1.0",
  "description": "Vue component library styled with Uno css",
  "main": "./dist/daim-ui.umd.js",
  "module": "./dist/daim-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/daim-ui.es.js",
      "require": "./dist/daim-ui.umd.js"
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore .",
    "lint:fix": "yarn run lint --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daim-dev/daim-ui.git"
  },
  "keywords": [
    "vue",
    "vite",
    "unocss",
    "component",
    "library"
  ],
  "author": "Damien Robinson <damien.robinson@daim.dev> (https://daim.dev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daim-dev/daim-ui/issues"
  },
  "homepage": "https://github.com/daim-dev/daim-ui#readme",
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@iconify-json/fa6-solid": "1.1.3",
    "mailgo": "0.12.2",
    "vue-demi": "0.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@mdx-js/react": "^1.6.22",
    "@nuxtjs/eslint-config": "9.0.0",
    "@nuxtjs/eslint-config-typescript": "9.0.0",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-docs": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/builder-vite": "0.1.26",
    "@storybook/vue3": "^6.4.21",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "@unocss/reset": "0.30.11",
    "@vitejs/plugin-vue": "2.3.1",
    "@vue/eslint-config-standard": "6.1.0",
    "babel-loader": "^8.2.4",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-nuxt": "3.2.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.5.0",
    "prettier": "2.6.2",
    "typescript": "4.5.4",
    "unocss": "0.30.12",
    "unplugin-vue-components": "0.18.5",
    "vite": "2.9.1",
    "vite-plugin-inspect": "0.4.3",
    "vue": "3.2.31",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.29.8",
    "vue2": "npm:vue@2"
  },
  "packageManager": "yarn@3.2.0"
}
